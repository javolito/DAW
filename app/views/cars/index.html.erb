<h1>Listing cars</h1>

<table>
  <thead>
    <tr>
      <th>Propietario</th>
      <th>Marca</th>
      <th>Modelo</th>
      <th>Año</th>
      <th>Combustible</th>
      <th>Cambio</th>
      <th>Km</th>
      <th>Descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cars.each do |car| %>
      <tr>
        <td><%= car.propietario %></td>
        <td><%= car.marca %></td>
        <td><%= car.modelo %></td>
        <td><%= car.año %></td>
        <td><%= car.combustible %></td>
        <td><%= car.cambio %></td>
        <td><%= car.km %></td>
        <td><%= car.descripcion %></td>
        <td><%= link_to 'Show', car %></td>
        <td><%= link_to 'Edit', edit_car_path(car) %></td>
        <td><%= link_to 'Destroy', car, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Car', new_car_path %>
